<Dataset
         name="example"
         dapVersion="4.0"
         dmrVersion="1.0"
         xmlns="http://xml.opendap.org/ns/DAP/4.0#"
         xmlns:dap="http://xml.opendap.org/ns/DAP/4.0#">
    <Dimension name="d1" size="10"/>
    <Dimension name="lat" size="20"/>
    <Dimension name="lon" size="20"/>


    <Enumeration name="e1" basetype="UInt8">
        <EnumConst name="econst1" value="1"/>
        <EnumConst name="econst2" value="2"/>
    </Enumeration>
    <Int32 name="x"/>
    <Float32 name="lat">
        <Dim name="/lat"/>
    </Float32>
    <Float32 name="lon">
        <Dim name="/lon"/>
    </Float32>
    <Sequence name="y">
        <Structure name="y">
            <Opaque name="f1"/>
            <Dim size="17"/>
            <Attribute name="a1" type="Int32">
                <Value value="5"/>
            </Attribute>
            <Attribute name="a2" type="Float64">
                <Value value="7.5"/>
                <Value value="10"/>
            </Attribute>
            <Attribute name="a3" type="String">
                <Value value="abcd &quot;hello&quot; &quot;quoted&quot;"/>
            </Attribute>
        </Structure>
        <Dim name="/d1"/>
        <Dim size="10"/>
    </Sequence>
    <Int32 name="funny">
        <Dim name="/g\.@\/\\$/lon\/2\.&quot;"/>
    </Int32>
    <Group name="g1">
        <Dimension name="d1" size="10"/>
        <Dimension name="d2" size="20"/>
        <Float32 name="tempb">
            <Dim name="/lat"/>
            <Dim name="/lon"/>
            <Dim name="/g1/d2"/>
        </Float32>
        <Enum name="evar" enum="/e1"/>
    </Group>
    <Group name="g.@/\$">
        <Dimension name="lon/2.&quot;" size="20"/>
    </Group>
</Dataset>
